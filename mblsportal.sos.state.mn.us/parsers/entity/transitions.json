[
  {
    "name": "collecttion:start",
    "trigger": {
      "type": "contains",
      "value": "Word or Phrase"
    },
    "sets": {
      "state": "collect"
    }
  },

  {
    "name": "collecttion:start",
    "trigger": {
      "type": "contains",
      "value": "Minnesota Business Name"
    },
    "sets": {
      "state": "collect"
    }
  },
  {
    "name": "collection:end",
    "trigger": {
      "type": "startsWith",
      "value": "<footer id=\"footer-site\">"
    },
    "sets": {
      "state": "scan"
    }
  },

  {
    "name": "gather:name",
    "states": ["collect"],
    "trigger": {
      "type": "startsWith",
      "value": "<span class=\"navbar-brand\""
    },
    "action": "gather:name",
    "sets": {
      "state": "scan"
    }
  },
 
  {
    "name": "row:data",
    "states": ["scan"],
    "trigger": {
      "type": "startsWith",
      "value": "<div id=\"filingSummary"
    },
    "sets": {
      "state": "row:data"
    }
  },

  {
    "name": "map:row",
    "states": ["row:data"],
    "trigger": {
      "type": "startsWith",
      "value": "<div class=\"row\">"
    },
    "action": "map:row",
    "sets": {
      "state": "gather:row"
    }
  },

  {
    "name": "row:collection",
    "states": ["gather:row"],
    "trigger": {
      "type": "startsWith",
      "value": "<dl>"
    },
    "sets": {
      "state": "dl"
    }
  },


  {
    "name": "row:key:collection",
    "states": ["dl"],
    "trigger": {
      "type": "startsWith",
      "value": "<dt>"
    },
    "sets": {
      "state": "dt"
    }
  },
  {
    "name": "row:key:collection:end",
    "states": ["dt"],
    "trigger": {
      "type": "startsWith",
      "value": "</dt>"
    },
    "sets": {
      "state": "gather:row"
    }
  },


  {
    "name": "row:data:collection",
    "states": ["gather:row"],
    "trigger": {
      "type": "startsWith",
      "value": "<dd>"
    },
    "sets": {
      "state": "dd"
    }
  },
  {
    "name": "row:data:collection:end",
    "states": ["dd"],
    "trigger": {
      "type": "startsWith",
      "value": "</dl>"
    },
    "action": "row:data:set",
    "sets": {
      "state": "gather:row"
    }
  },
  {
    "name": "row:key:collect",
    "states": ["dt"],
    "trigger": {
      "type": "noTag"
    },
    "action": "row:initialize:next_key"
  },

  {
    "name": "row:data:collect",
    "states": ["dd"],
    "trigger": {
      "type": "noTag"
    },
    "action": "row:gather:next_value"
  },

  {
    "name": "row:data:collection:address",
    "states": ["dd"],
    "trigger": {
      "type": "exact",
      "value": "<address>"
    },
    "sets": {
      "state": "row:data:collection:address"
    }
  },

  {
    "name": "row:data:collect:address",
    "states": ["row:data:collection:address"],
    "trigger": {
      "type": "noTag"
    },
    "action": "row:gather:next_value"
  },

  {
    "name": "row:data:collection:address:end",
    "states": ["row:data:collection:address"],
    "trigger": {
      "type": "startsWith",
      "value": "</address>"
    },
    "sets": {
      "state": "gather:row"
    },
    "action": "row:address:set"
  },
  {
    "name": "row:finished",
    "states": ["gather:row","row:collection", "dl", "dt", "dd"],
    "trigger": {
      "type": "startsWith",
      "value": "<a name=\"orderselected\"></a>"
    },
    "sets": {
      "state": "collect"
    }
  },

  {
    "name": "other_info",
